.profile-settings {
    &-item {
        display: flex;
        justify-content: space-between;
        border-bottom: 1px solid var(--graySecondary);
        padding-bottom: 10px;
    }

    &-label {
        color: var(--grayPrimary);
    }

    &-value {
        color: var(--whiteSecondary);
    }
}
